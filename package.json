{
  "name": "tasker",
  "displayName": "Tasker",
  "description": "Execute the given task and return the output",
  "version": "0.1.1",
  "publisher": "vishalwy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vishalwy/vsc-tasker.git"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "task",
    "input command variable",
    "task output",
    "configure tasks"
  ],
  "activationEvents": [
    "*",
    "onCommand:tasker.run",
    "onCommand:tasker.configure"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "package": "vsce package"
  },
  "dependencies": {
    "@types/mustache": "^0.8.32",
    "mustache": "^2.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vsce": "^1.61.0",
    "vscode": "^1.1.32"
  }
}
